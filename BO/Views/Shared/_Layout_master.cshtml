<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Dashboard" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- Bootstrap Core CSS -->
    <link href="/Content/bootstrap.min.css" rel='stylesheet' type='text/css' />
    <!-- Custom CSS -->
    <link href="/Content/style1.css" rel='stylesheet' type='text/css' />

    <!-- Graph CSS -->
    <link href="/Content/font-awesome1.css" rel="stylesheet">
    <!-- jQuery -->
    <link href='fonts.googleapis.com/css?family=Roboto:700,500,300,100italic,100,400' rel='stylesheet' type='text/css' />
    <link href='fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>
    <!-- lined-icons -->
    <link rel="stylesheet" href="/css/icon-font.min.css" type='text/css' />
    <script src="/Scripts/js/amcharts.js"></script>
    <script src="/Scripts/js/serial.js"></script>
    <script src="/Scripts/js/light.js"></script>
    <!-- //lined-icons -->
    <script src="/Scripts/js/jquery-1.10.2.min.js"></script>
    <!--pie-chart--->
    <script src="/Scripts/js/pie-chart.js" type="text/javascript"></script>
    <script type="text/javascript">

        $(document).ready(function () {
            $('#demo-pie-1').pieChart({
                barColor: '#3bb2d0',
                trackColor: '#eee',
                lineCap: 'round',
                lineWidth: 8,
                onStep: function (from, to, percent) {
                    $(this.element).find('.pie-value').text(Math.round(percent) + '%');
                }
            });

            $('#demo-pie-2').pieChart({
                barColor: '#fbb03b',
                trackColor: '#eee',
                lineCap: 'butt',
                lineWidth: 8,
                onStep: function (from, to, percent) {
                    $(this.element).find('.pie-value').text(Math.round(percent) + '%');
                }
            });

            $('#demo-pie-3').pieChart({
                barColor: '#ed6498',
                trackColor: '#eee',
                lineCap: 'square',
                lineWidth: 8,
                onStep: function (from, to, percent) {
                    $(this.element).find('.pie-value').text(Math.round(percent) + '%');
                }
            });


        });

    </script>
</head>
<body>
    <div>
        @RenderBody()
    </div>
    <div class="page-container">
        <!--/content-inner-->
        <!--//content-inner-->
        <!--/sidebar-menu-->
        <div class="sidebar-menu">
            <header class="logo1">
                <a href="#" class="sidebar-icon"> <span class="fa fa-bars"></span> </a>
            </header>
            <div style="border-top:1px ridge rgba(255, 255, 255, 0.15)"></div>
            <div class="menu">
                <ul id="menu">
                    <li><a href="Dashboard"><i></i> <span>Dashboard</span></a></li>
                    <li id="menu-academico">
                        <a href="#"><i></i> <span> Master</span> <span class="fa fa-angle-right" style="float: right"></span></a>
                        <ul id="menu-academico-sub">
                            <li id="menu-academico-avaliacoes"><a href="CountryMaster">Country Master</a></li>
                            <li id="menu-academico-avaliacoes"><a href="CityMaster">City Master</a></li>
                            <li id="menu-academico-boletim"><a href="AreaMaster">Area Master</a></li>
                        </ul>
                    </li>


                    <li id="menu-academico">
                        <a href="#"><i></i> <span>Vendor</span> <span class="fa fa-angle-right" style="float: right"></span></a>
                        <ul id="menu-academico-sub">
                            <li id="menu-academico-avaliacoes"><a href="AddVendor">Add Vendor</a></li>
                            <li id="menu-academico-boletim"><a href="PendingRequest">Pending Request</a></li>

                        </ul>
                    </li>
                    <li>
                        <a href="#"><i></i> <span>Reports</span> <span class="fa fa-angle-right" style="float: right"></span></a>
                        <ul>
                            <li><a href="#"> Transaction Reports</a></li>

                        </ul>
                    </li>
                    <li id="menu-academico"><a href="Index"><i></i> <span>Logout</span></a></li>
                </ul>
            </div>
        </div>
        <div class="clearfix"></div>
    </div>
    <script>
                                    var toggle = true;

                                    $(".sidebar-icon").click(function () {
                                        if (toggle) {
                                            $(".page-container").addClass("sidebar-collapsed").removeClass("sidebar-collapsed-back");
                                            $("#menu span").css({ "position": "absolute" });
                                        }
                                        else {
                                            $(".page-container").removeClass("sidebar-collapsed").addClass("sidebar-collapsed-back");
                                            setTimeout(function () {
                                                $("#menu span").css({ "position": "relative" });
                                            }, 400);
                                        }

                                        toggle = !toggle;
                                    });
    </script>
    <!--js -->
    <script src="Scripts/jquery.nicescroll.js"></script>
    <script src="Scripts/scripts.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="Scripts/bootstrap.min.js"></script>
    <!-- /Bootstrap Core JavaScript -->
    <!-- real-time -->
    <script language="javascript" type="text/javascript" src="Scripts/jquery.flot.js"></script>

    <!-- /real-time -->
    <script src="Scripts/jquery.fn.gantt.js"></script>

    <script src="Scripts/menu_jquery.js"></script>
</body>
</html>
