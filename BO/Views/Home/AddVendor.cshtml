
@{
    ViewBag.Title = "AddVendor";
    Layout = "~/Views/Shared/_Layout_master.cshtml";
}

<div class="page-container">
    <!--/content-inner-->
    <div class="left-content">
        <div class="inner-content">
            <div class="header-section">
                <!-- top_bg -->
                <div class="top_bg">

                    <div class="header_top">
                        <div class="top_right">
                            <ul>
                                <li><a href="#">help</a></li>|
                                <li><a href="#">Contact</a></li>|
                                <li><a href="#">Delivery information</a></li>
                            </ul>
                        </div>
                        <div class="top_left">
                            <h2><span></span> Call us : 032 2352 782</h2>
                        </div>
                        <div class="clearfix"> </div>
                    </div>

                </div>
                <div class="clearfix"></div>
                <!-- /top_bg -->
            </div>
            <div class="content">
                <div class="area">
                    <div class="col-md-6" style="margin-left:25%;">
                        <div class="forms">
                            <div class="form-grids widget-shadow">
                                <div class="form-title">
                                    <h4>Add Vendor</h4>
                                </div>
                                <div class="form-body">
                                    <form>
                                        <div class="form-group"> <label for="exampleInputEmail1">Restaurant ID</label> <input type="text" class="form-control" id="txt_rid" placeholder="Restaurant Id" /> </div>
                                        <div class="form-group"> <label for="exampleInputEmail1">Restaurant Name</label> <input type="text" class="form-control" id="txt_rname" placeholder="Restaurant Name" /> </div>
                                        <div class="form-group"> <label for="exampleInputEmail1">Restaurant Address</label> <textarea class="form-control" id="txt_raddress" placeholder="Restaurant Address"rows="4" cols="5"></textarea> </div>
                                        <div class="form-group"> <label for="exampleInputEmail1">Country Name</label> <select class="form-control" id="txt_cname" placeholder="Country Name"> </select></div>
                                        <div class="form-group"> <label for="exampleInputEmail1">City Name</label> <select class="form-control" id="txt_ciname" placeholder="City Name"></select> </div>
                                        <div class="form-group"> <label for="exampleInputEmail1">Area Name</label> <select class="form-control" id="txt_aname" placeholder="Area Name"></select> </div>
                                        <div class="form-group"> <label for="exampleInputEmail1">Restaurant Licence No</label> <input type="text" class="form-control" id="txt_lic" placeholder="Restaurant Licence No" /> </div>
                                        <div class="form-group"> <label for="exampleInputEmail1">Restaurant Contact</label> <input type="text" class="form-control" id="txt_rcnt" placeholder="Restaurant Contact" /> </div>
                                        <div class="form-group"> <label for="exampleInputEmail1">Restaurant Mail Id</label> <input type="text" class="form-control" id="txt_lic" placeholder="Restaurant Mail Id" /> </div>
                                        <div class="form-group"> <label for="exampleInputFile">Licence File</label> <input type="file" id="exampleInputFile"> </div>
                                        <div class="form-group"> <label for="exampleInputEmail1">Properiator Name</label> <input type="text" class="form-control" id="txt_pname" placeholder="Properiator Name" /> </div>
                                        <div class="form-group"> <label for="exampleInputEmail1">Properiator Contact</label> <input type="text" class="form-control" id="txt_pcnt" placeholder="Properiator Contact" /> </div>
                                        <div class="form-group"> <label for="exampleInputEmail1">Properiator Mail</label> <input type="text" class="form-control" id="txt_pmail" placeholder="Properiator Mail" /> </div>
                                        <div class="checkbox"> <label> <input type="checkbox"> Active </label> </div>
                                        <button type="submit" class="btn btn-default">Submit</button>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>



                    <div class="clearfix"></div>
                </div>

            </div>
        </div>

    </div>
    <input style="margin-left:50%;visibility:hidden;" type="text" id="txt_id" />
</div>

